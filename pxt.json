{
    "name": "pxt-bleadv-common",
    "description": "Common files for other pxt-bleadv-* extensions providing BLEAdvManager and CryptTools. micro:bit (V2) only.",
    "dependencies": {
        "core": "*"
    },
    "files": [
        "README.md",
        "inc/BLEAdvManager.h",
        "inc/IBLEAdvClient.h",
        "inc/CryptTool.h",
        "source/BLEAdvManager.cpp",
        "source/CryptTool.cpp"
    ],
    "targetVersions": {
        "target": "7.0.57",
        "targetId": "microbit"
    },
    "supportedTargets": [
        "microbit"
    ],
    "disablesVariants": [
        "mbdal"
    ],
    "yotta": {
        "config": {
            "microbit-dal": {
                "bluetooth": {
                    "enabled": 1,
                    "eddystone_url": 0,
                    "eddystone_uid": 0
                }
            }
        }
    }
}
